{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "prefer-const": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 0,
    "prefer-template": 1,
    "max-len": 1
  }
}
